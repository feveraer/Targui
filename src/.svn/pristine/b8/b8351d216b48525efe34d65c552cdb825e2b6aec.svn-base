package domein;

public class Spel {

    private final Spelbord spelbord = new Spelbord();

    public Vak[][] geefSpel() {
        return spelbord.geefSpelbord();
    }
        
        

    public Spelbord getSpelbord() {
        return spelbord;
    }
    
    public void vulSpelbord()
    {
        spelbord.vulSpelbord();
    }
    
    public void plaatsNederzetting(Speler speler, Vak vak)
    {
        spelbord.plaatsNederzetting(speler, vak);
    }

}
